<?xml version="1.0"?>
<cd:ProductDefinition id="AUX_RBC_L2_01_20" format="binary" last-modified="2008-07-01" xmlns:cd="http://www.stcorp.nl/coda/definition/2008/07" xmlns:ct="http://www.stcorp.nl/coda/test/2008/10">
  <cd:Record format="binary">
    <cd:Description>ADM-Aeolus Rayleigh-Brillouin Correction (RBC) look-up tables</cd:Description>
    <cd:Field name="mph">
      <cd:NamedType id="MPH_v1"/>
    </cd:Field>
    <cd:Field name="sph">
      <cd:NamedType id="AUX_RBC_SPH_01_20"/>
    </cd:Field>
    <cd:Field name="dsd">
      <cd:Array format="ascii">
        <cd:Dimension><![CDATA[$num_dsd]]></cd:Dimension>
        <cd:NamedType id="DSD"/>
      </cd:Array>
    </cd:Field>
    <cd:Field name="rayleigh_brillouin">
      <cd:Array>
        <cd:Description>Rayleigh Brillouin look-up tables</cd:Description>
        <cd:Dimension>1</cd:Dimension>
        <cd:NamedType id="Rayleigh_Brillouin_ADS"/>
      </cd:Array>
    </cd:Field>
  </cd:Record>
  <cd:ProductVariable name="num_dsd">
    <cd:Init><![CDATA[$num_dsd = int(/mph/num_dsd)]]></cd:Init>
  </cd:ProductVariable>

