    <cd:Type namexml="ACCD_Edge_Length">
      <cd:Attribute name="unit">
        <cd:FixedValue>cm</cd:FixedValue>
      </cd:Attribute>
      <cd:Float format="ascii">
        <cd:Description>Edge length of one pixel of the ACCD.</cd:Description>
        <cd:NativeType>double</cd:NativeType>
      </cd:Float>
    </cd:Type>
  </cd:Field>
  <cd:Field name="Mie_Gradient_Correction_Slope">
    <cd:Type namexml="Mie_Gradient_Correction_Slope">
      <cd:Float format="ascii">
        <cd:Description>Slope used for the Mie horizontal gradient correction</cd:Description>
        <cd:NativeType>double</cd:NativeType>
      </cd:Float>
    </cd:Type>
  </cd:Field>
  <cd:Field name="Mie_Gradient_Correction_Offset">
    <cd:Type namexml="Mie_Gradient_Correction_Offset">
      <cd:Float format="ascii">
        <cd:Description>Offset used for the Mie horizontal gradient correction</cd:Description>
        <cd:NativeType>double</cd:NativeType>
