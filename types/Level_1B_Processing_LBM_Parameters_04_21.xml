    <cd:Type namexml="Image_Column_Channel2_Min">
      <cd:Integer format="ascii">
        <cd:Description>Minimum column attributed to channel 1.</cd:Description>
        <cd:NativeType>uint16</cd:NativeType>
      </cd:Integer>
    </cd:Type>
  </cd:Field>
