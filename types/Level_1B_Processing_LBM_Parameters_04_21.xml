<?xml version="1.0"?>
<cd:Record xmlns:cd="http://www.stcorp.nl/coda/definition/2008/07" xmlns:ct="http://www.stcorp.nl/coda/test/2008/10" format="xml" last-modified="2025-12-15" name="Level_1B_Processing_LBM_Parameters_04_21" namexml="Level_1B_Processing_LBM_Parameters">
  <cd:Description>Processing parameters for Laser Beam Monitoring Calibration</cd:Description>
  <cd:Field name="File_Type">
    <cd:Text namexml="File_Type">
      <cd:Description>File type string</cd:Description>
    </cd:Text>
  </cd:Field>
  <cd:Field name="Sph_Descriptor">
    <cd:Text namexml="Sph_Descriptor">
      <cd:Description>ASCII string describing the product</cd:Description>
    </cd:Text>
  </cd:Field>
  <cd:Field name="Mie_Image_Binarization_Factor">
    <cd:Type namexml="Mie_Image_Binarization_Factor">
      <cd:Float format="ascii">
        <cd:Description>Factor used to calculate the binarization threshold</cd:Description>
        <cd:NativeType>double</cd:NativeType>
      </cd:Float>
    </cd:Type>
  </cd:Field>
  <cd:Field name="Max_Iterations_Ellipse_Fit">
    <cd:Type namexml="Max_Iterations_Ellipse_Fit">
      <cd:Integer format="ascii">
        <cd:Description>Maximum iterations of least squares ellipse fit</cd:Description>
        <cd:NativeType>uint16</cd:NativeType>
      </cd:Integer>
    </cd:Type>
  </cd:Field>
  <cd:Field name="Step_Size_Factor">
    <cd:Type namexml="Step_Size_Factor">
      <cd:Float format="ascii">
        <cd:Description>Factor used to vary diagonal elements of transmission matrix</cd:Description>
        <cd:NativeType>double</cd:NativeType>
      </cd:Float>
    </cd:Type>
  </cd:Field>
  <cd:Field name="Max_Step_Factor">
    <cd:Type namexml="Max_Step_Factor">
      <cd:Float format="ascii">
        <cd:Description>Maximum variation of diagonal elements of transmission matrix</cd:Description>
        <cd:NativeType>double</cd:NativeType>
      </cd:Float>
    </cd:Type>
  </cd:Field>
  <cd:Field name="Min_Step_Factor">
    <cd:Type namexml="Min_Step_Factor">
      <cd:Float format="ascii">
        <cd:Description>Minimum variation of diagonal elements of transmission matrix</cd:Description>
        <cd:NativeType>double</cd:NativeType>
      </cd:Float>
    </cd:Type>
  </cd:Field>
  <cd:Field name="Stop_Tolerance">
    <cd:Type namexml="Stop_Tolerance">
      <cd:Float format="ascii">
        <cd:Description>Tolerance to check convergence of least squares error</cd:Description>
        <cd:NativeType>double</cd:NativeType>
      </cd:Float>
    </cd:Type>
  </cd:Field>
  <cd:Field name="Start_Frequency_Ramp">
    <cd:Type namexml="Start_Frequency_Ramp">
      <cd:Attribute name="unit">
        <cd:FixedValue>GHz</cd:FixedValue>
      </cd:Attribute>
      <cd:Float format="ascii">
        <cd:Description>Lower frequency threshold for frequency steps to be used for the calibration result calculation.</cd:Description>
        <cd:NativeType>double</cd:NativeType>
      </cd:Float>
    </cd:Type>
  </cd:Field>
  <cd:Field name="Stop_Frequency_Ramp">
    <cd:Type namexml="Stop_Frequency_Ramp">
      <cd:Attribute name="unit">
        <cd:FixedValue>GHz</cd:FixedValue>
      </cd:Attribute>
      <cd:Float format="ascii">
        <cd:Description>Upper frequency threshold for frequency steps to be used for the calibration result calculation.</cd:Description>
        <cd:NativeType>double</cd:NativeType>
      </cd:Float>
    </cd:Type>
  </cd:Field>
  <cd:Field name="Min_Num_Valid_Images">
    <cd:Type namexml="Min_Num_Valid_Images">
      <cd:Integer format="ascii">
        <cd:Description>Minimum number of valid images required to assume frequency step results valid.</cd:Description>
        <cd:NativeType>uint16</cd:NativeType>
      </cd:Integer>
    </cd:Type>
  </cd:Field>
