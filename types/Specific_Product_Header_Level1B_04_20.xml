<?xml version="1.0"?>
<cd:Record xmlns:cd="http://www.stcorp.nl/coda/definition/2008/07" xmlns:ct="http://www.stcorp.nl/coda/test/2008/10" format="xml" last-modified="2025-01-15" name="Specific_Product_Header_Level1B_04_20" namexml="Specific_Product_Header">
  <cd:Description>Specific Product Header for ALD Level 1B products</cd:Description>
  <cd:Field name="Sph_Descriptor">
    <cd:Text namexml="Sph_Descriptor">
    </cd:Text>
  </cd:Field>
  <cd:Field name="DOI">
    <cd:Text namexml="DOI">
    </cd:Text>
  </cd:Field>
  <cd:Field name="Intersect_Start_Lat">
    <cd:Type namexml="Intersect_Start_Lat">
      <cd:Attribute name="unit">
        <cd:Optional/>
        <cd:FixedValue>10-6DegN</cd:FixedValue>
      </cd:Attribute>
      <cd:Integer format="ascii">
        <cd:Description>Latitude of the intersection of WGS84 DEM and the satellite line-of-sight for the first measurement (first measurement centroid time of the first BRC)</cd:Description>
        <cd:Unit>1e-6 degrees_north</cd:Unit>
        <cd:Conversion denominator="1000000.000000" numerator="1.000000">
          <cd:Unit>degrees_north</cd:Unit>
        </cd:Conversion>
        <cd:NativeType>int32</cd:NativeType>
        <ct:NamedTest id="RangeLatitude6"/>
      </cd:Integer>
    </cd:Type>
  </cd:Field>
  <cd:Field name="Intersect_Start_Long">
    <cd:Type namexml="Intersect_Start_Long">
      <cd:Attribute name="unit">
        <cd:Optional/>
        <cd:FixedValue>10-6DegE</cd:FixedValue>
      </cd:Attribute>
      <cd:Integer format="ascii">
        <cd:Description>Longitude of the intersection of WGS84 DEM and the satellite line-of-sight for the first measurement (first measurement centroid time of the first BRC)</cd:Description>
        <cd:Unit>1e-6 degrees_east</cd:Unit>
        <cd:Conversion denominator="1000000.000000" numerator="1.000000">
          <cd:Unit>degrees_east</cd:Unit>
        </cd:Conversion>
        <cd:NativeType>int32</cd:NativeType>
        <ct:NamedTest id="RangeLongitude6"/>
      </cd:Integer>
    </cd:Type>
  </cd:Field>
  <cd:Field name="Intersect_Stop_Lat">
    <cd:Type namexml="Intersect_Stop_Lat">
      <cd:Attribute name="unit">
        <cd:Optional/>
        <cd:FixedValue>10-6DegN</cd:FixedValue>
      </cd:Attribute>
      <cd:Integer format="ascii">
        <cd:Description>Latitude of the intersection of WGS84 DEM and the satellite line-of-sight for the last measurement (last measurement centroid time of the last BRC)</cd:Description>
        <cd:Unit>1e-6 degrees_north</cd:Unit>
        <cd:Conversion denominator="1000000.000000" numerator="1.000000">
          <cd:Unit>degrees_north</cd:Unit>
        </cd:Conversion>
        <cd:NativeType>int32</cd:NativeType>
        <ct:NamedTest id="RangeLatitude6"/>
      </cd:Integer>
    </cd:Type>
  </cd:Field>
  <cd:Field name="Intersect_Stop_Long">
    <cd:Type namexml="Intersect_Stop_Long">
      <cd:Attribute name="unit">
        <cd:Optional/>
        <cd:FixedValue>10-6DegE</cd:FixedValue>
      </cd:Attribute>
      <cd:Integer format="ascii">
        <cd:Description>Longitude of the intersection of WGS84 DEM and the satellite line-of-sight for the last measurement (last measurement centroid time of the last BRC)</cd:Description>
        <cd:Unit>1e-6 degrees_east</cd:Unit>
        <cd:Conversion denominator="1000000.000000" numerator="1.000000">
          <cd:Unit>degrees_east</cd:Unit>
        </cd:Conversion>
        <cd:NativeType>int32</cd:NativeType>
        <ct:NamedTest id="RangeLongitude6"/>
      </cd:Integer>
    </cd:Type>
  </cd:Field>
  <cd:Field name="Sat_Track">
    <cd:Type namexml="Sat_Track">
      <cd:Attribute name="unit">
        <cd:Optional/>
        <cd:FixedValue>deg</cd:FixedValue>
      </cd:Attribute>
      <cd:Float format="ascii">
        <cd:Description>Sub-satellite heading (nadir) at the sensing start time in the MPH</cd:Description>
        <cd:Unit>degrees</cd:Unit>
        <cd:NativeType>double</cd:NativeType>
        <ct:NamedTest id="FloatIsFinite"/>
      </cd:Float>
    </cd:Type>
  </cd:Field>
  <cd:Field name="Spare_1">
    <cd:Hidden/>
    <cd:Text namexml="Spare_1">
    </cd:Text>
  </cd:Field>
  <cd:Field name="Base_Laser_Frequency">
    <cd:Type namexml="Base_Laser_Frequency">
      <cd:Attribute name="unit">
        <cd:Optional/>
        <cd:FixedValue>GHz</cd:FixedValue>
      </cd:Attribute>
      <cd:Float format="ascii">
        <cd:Description>Base Laser Transmit Frequency</cd:Description>
        <cd:Unit>GHz</cd:Unit>
        <cd:NativeType>double</cd:NativeType>
        <ct:NamedTest id="FloatIsFinite"/>
      </cd:Float>
    </cd:Type>
  </cd:Field>
  <cd:Field name="N_Max">
    <cd:Type namexml="N_Max">
      <cd:Integer format="ascii">
        <cd:Description>Measurement array size of the output L1B product. This value is the maximum number of measurements per BRC in the input Level 1A data</cd:Description>
        <cd:NativeType>int32</cd:NativeType>
      </cd:Integer>
    </cd:Type>
  </cd:Field>
  <cd:Field name="N_Max_Actual">
    <cd:Type namexml="N_Max_Actual">
      <cd:Integer format="ascii">
        <cd:Description>Actual maximum number of measurements per BRC in Level 1B output product</cd:Description>
        <cd:NativeType>int32</cd:NativeType>
      </cd:Integer>
    </cd:Type>
  </cd:Field>
  <cd:Field name="Total_Num_of_Observations">
    <cd:Type namexml="Total_Num_of_Observations">
      <cd:Integer format="ascii">
        <cd:Description>Number of observations</cd:Description>
        <cd:NativeType>int32</cd:NativeType>
      </cd:Integer>
    </cd:Type>
  </cd:Field>
  <cd:Field name="Total_Num_of_Measurements">
    <cd:Type namexml="Total_Num_of_Measurements">
      <cd:Integer format="ascii">
        <cd:Description>Number of total measurements</cd:Description>
        <cd:NativeType>int32</cd:NativeType>
      </cd:Integer>
    </cd:Type>
  </cd:Field>
  <cd:Field name="Total_Num_of_Reference_Pulses">
    <cd:Type namexml="Total_Num_of_Reference_Pulses">
      <cd:Integer format="ascii">
        <cd:Description>Number of total reference pulses</cd:Description>
        <cd:NativeType>int32</cd:NativeType>
      </cd:Integer>
    </cd:Type>
  </cd:Field>
  <cd:Field name="Spare_2">
    <cd:Hidden/>
    <cd:Text namexml="Spare_2">
    </cd:Text>
  </cd:Field>
  <cd:Field name="Num_Mie_Observations_Used">
    <cd:Type namexml="Num_Mie_Observations_Used">
      <cd:Integer format="ascii">
        <cd:Description>Number of observations used for Mie wind velocity estimates (24 estimates, one per altitude bin)</cd:Description>
        <cd:NativeType>int32</cd:NativeType>
      </cd:Integer>
    </cd:Type>
  </cd:Field>
  <cd:Field name="Num_Rayleigh_Observations_Used">
    <cd:Type namexml="Num_Rayleigh_Observations_Used">
      <cd:Integer format="ascii">
        <cd:Description>Number of observations used for Rayleigh wind velocity estimates (24 estimates, one per altitude bin)</cd:Description>
        <cd:NativeType>int32</cd:NativeType>
      </cd:Integer>
    </cd:Type>
  </cd:Field>
  <cd:Field name="Num_Mie_Measurements_Used">
    <cd:Type namexml="Num_Mie_Measurements_Used">
      <cd:Integer format="ascii">
        <cd:Description>Number of measurements used for Mie wind velocity estimates (24 estimates, one per altitude bin)</cd:Description>
        <cd:NativeType>int32</cd:NativeType>
      </cd:Integer>
    </cd:Type>
  </cd:Field>
  <cd:Field name="Num_Rayleigh_Measurements_Used">
    <cd:Type namexml="Num_Rayleigh_Measurements_Used">
      <cd:Integer format="ascii">
        <cd:Description>Number of Rayleigh measurements used for Rayleigh wind velocity estimates (24 estimates, one per altitude bin)</cd:Description>
        <cd:NativeType>int32</cd:NativeType>
      </cd:Integer>
    </cd:Type>
  </cd:Field>
  <cd:Field name="Num_Mie_Reference_Pulses_Used">
    <cd:Type namexml="Num_Mie_Reference_Pulses_Used">
      <cd:Integer format="ascii">
        <cd:Description>Number of Mie reference pulses used</cd:Description>
        <cd:NativeType>int32</cd:NativeType>
      </cd:Integer>
    </cd:Type>
  </cd:Field>
  <cd:Field name="Num_Rayleigh_Reference_Pulses_Used">
    <cd:Type namexml="Num_Rayleigh_Reference_Pulses_Used">
      <cd:Integer format="ascii">
        <cd:Description>Number of Rayleigh reference pulses used</cd:Description>
        <cd:NativeType>int32</cd:NativeType>
      </cd:Integer>
    </cd:Type>
  </cd:Field>
  <cd:Field name="Spare_3">
    <cd:Hidden/>
    <cd:Text namexml="Spare_3">
    </cd:Text>
  </cd:Field>
  <cd:Field name="Num_Mie_Zero_Wind_Detected">
    <cd:Type namexml="Num_Mie_Zero_Wind_Detected">
      <cd:Integer format="ascii">
        <cd:Description>Number of observations in which Mie zero wind is detected(a new ground correction factor calculated)</cd:Description>
        <cd:NativeType>int32</cd:NativeType>
      </cd:Integer>
    </cd:Type>
  </cd:Field>
  <cd:Field name="Num_Rayleigh_Zero_Wind_Detected">
    <cd:Type namexml="Num_Rayleigh_Zero_Wind_Detected">
      <cd:Integer format="ascii">
        <cd:Description>Number of observations containing Rayleigh zero wind is detected (a new ground correction factor calculated)</cd:Description>
        <cd:NativeType>int32</cd:NativeType>
      </cd:Integer>
    </cd:Type>
  </cd:Field>
  <cd:Field name="Num_Mie_Measurements_Ground_Echo_Detected">
    <cd:Type namexml="Num_Mie_Measurements_Ground_Echo_Detected">
      <cd:Integer format="ascii">
        <cd:Description>Total number of Mie measurements where ground wind bins were detected</cd:Description>
        <cd:NativeType>int32</cd:NativeType>
      </cd:Integer>
    </cd:Type>
  </cd:Field>
  <cd:Field name="Num_Rayleigh_Measurements_Ground_Echo_Detected">
    <cd:Type namexml="Num_Rayleigh_Measurements_Ground_Echo_Detected">
      <cd:Integer format="ascii">
        <cd:Description>Total number of Rayleigh measurements where ground wind bins were detected</cd:Description>
        <cd:NativeType>int32</cd:NativeType>
      </cd:Integer>
    </cd:Type>
  </cd:Field>
  <cd:Field name="Spare_4">
    <cd:Hidden/>
    <cd:Text namexml="Spare_4">
    </cd:Text>
  </cd:Field>
  <cd:Field name="Total_Num_of_Measurement_Invalid">
    <cd:Type namexml="Total_Num_of_Measurement_Invalid">
      <cd:Integer format="ascii">
        <cd:Description>Total number of measurements for which the number of corresponding valid pulses is below Meas_Cavity_Lock_Status_Thresh</cd:Description>
        <cd:NativeType>int32</cd:NativeType>
      </cd:Integer>
    </cd:Type>
  </cd:Field>
  <cd:Field name="Total_Num_of_Pulse_Validity_Status_Flag_False">
    <cd:Type namexml="Total_Num_of_Pulse_Validity_Status_Flag_False">
      <cd:Integer format="ascii">
        <cd:Description>Total number of reference pulses with Pulse_Validity_Status_Flag set to false</cd:Description>
        <cd:NativeType>int32</cd:NativeType>
      </cd:Integer>
    </cd:Type>
  </cd:Field>
  <cd:Field name="Total_Num_of_Sat_Not_on_Target_Measurements">
    <cd:Type namexml="Total_Num_of_Sat_Not_on_Target_Measurements">
      <cd:Integer format="ascii">
        <cd:Description>Total number of measurements with satellite not on target status</cd:Description>
        <cd:NativeType>int32</cd:NativeType>
      </cd:Integer>
    </cd:Type>
  </cd:Field>
  <cd:Field name="Total_Num_of_Corrupt_Mie_Measurement_Bins">
    <cd:Type namexml="Total_Num_of_Corrupt_Mie_Measurement_Bins">
      <cd:Integer format="ascii">
        <cd:Description>The total number of corrupt Mie measurement bins</cd:Description>
        <cd:NativeType>int32</cd:NativeType>
      </cd:Integer>
    </cd:Type>
  </cd:Field>
  <cd:Field name="Total_Num_of_Corrupt_Rayleigh_Measurement_Bins">
    <cd:Type namexml="Total_Num_of_Corrupt_Rayleigh_Measurement_Bins">
      <cd:Integer format="ascii">
        <cd:Description>The total number of corrupt Rayleigh measurement bins</cd:Description>
        <cd:NativeType>int32</cd:NativeType>
      </cd:Integer>
    </cd:Type>
  </cd:Field>
  <cd:Field name="Total_Num_of_Corrupt_Mie_Reference_Pulses">
    <cd:Type namexml="Total_Num_of_Corrupt_Mie_Reference_Pulses">
      <cd:Integer format="ascii">
        <cd:Description>The total number of corrupt Mie reference pulses</cd:Description>
        <cd:NativeType>int32</cd:NativeType>
      </cd:Integer>
    </cd:Type>
  </cd:Field>
  <cd:Field name="Total_Num_of_Corrupt_Rayleigh_Reference_Pulses">
    <cd:Type namexml="Total_Num_of_Corrupt_Rayleigh_Reference_Pulses">
      <cd:Integer format="ascii">
        <cd:Description>The total number of corrupt Rayleigh reference pulses</cd:Description>
        <cd:NativeType>int32</cd:NativeType>
      </cd:Integer>
    </cd:Type>
  </cd:Field>
  <cd:Field name="NF_Order">
    <cd:Type namexml="NF_Order">
      <cd:Integer format="ascii">
        <cd:Description>Number of Fourier terms used in the Harmonic Bias Estimation</cd:Description>
        <cd:NativeType>int32</cd:NativeType>
      </cd:Integer>
    </cd:Type>
  </cd:Field>
  <cd:Field name="Spare_5">
    <cd:Hidden/>
    <cd:Text namexml="Spare_5">
    </cd:Text>
  </cd:Field>
  <cd:Field name="List_of_Dsds">
    <cd:NamedType id="List_of_Dsds"/>
  </cd:Field>
</cd:Record>
