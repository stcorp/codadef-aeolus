<?xml version="1.0"?>
<cd:Record xmlns:cd="http://www.stcorp.nl/coda/definition/2008/07" xmlns:ct="http://www.stcorp.nl/coda/test/2008/10" format="xml" last-modified="2025-01-16" name="Level_1A_Processing_Parameters_04_20" namexml="Level_1A_Processing_Parameters">
  <cd:Description>Level 1A Processing Parameters GADS DSR</cd:Description>
  <cd:Field name="List_of_Data_Set_Records">
    <cd:Record namexml="List_of_Data_Set_Records">
      <cd:Description>List of Data Set Records</cd:Description>
      <cd:Attribute name="count">
      </cd:Attribute>
      <cd:Field name="Data_Set_Record">
        <cd:Array>
          <cd:Dimension/>
          <cd:Record namexml="Data_Set_Record">
            <cd:Description>Data Set Record</cd:Description>
            <cd:Field name="FH_Default_Fields">
              <cd:Record namexml="FH_Default_Fields">
                <cd:Description>Fields responsible for populating the Fixed Header</cd:Description>
                <cd:Field name="File_Description">
                  <cd:Text namexml="File_Description">
                    <cd:Description>1-line description of the file</cd:Description>
                  </cd:Text>
                </cd:Field>
                <cd:Field name="Notes">
                  <cd:Text namexml="Notes">
                    <cd:Description>Multi-lines free text</cd:Description>
                  </cd:Text>
                </cd:Field>
                <cd:Field name="Mission">
                  <cd:Text namexml="Mission">
                    <cd:Description>Aeolus</cd:Description>
                  </cd:Text>
                </cd:Field>
                <cd:Field name="Mission_Id">
                  <cd:Text namexml="Mission_Id">
                    <cd:Description>AE</cd:Description>
                  </cd:Text>
                </cd:Field>
                <cd:Field name="File_Class">
                  <cd:Text namexml="File_Class">
                    <cd:Description>1-Line description of the file class</cd:Description>
                  </cd:Text>
                </cd:Field>
                <cd:Field name="File_Version">
                  <cd:Type namexml="File_Version">
                    <cd:Integer format="ascii">
                      <cd:Description>4 digits used to distinguish between versions of a file having the same validity period</cd:Description>
                      <cd:NativeType>int16</cd:NativeType>
                    </cd:Integer>
                  </cd:Type>
                </cd:Field>
                <cd:Field name="System">
                  <cd:Text namexml="System">
                    <cd:Description>Aeolus L1bP</cd:Description>
                  </cd:Text>
                </cd:Field>
                <cd:Field name="Creator">
                  <cd:Text namexml="Creator">
                    <cd:Description>Aeolus L1bP</cd:Description>
                  </cd:Text>
                </cd:Field>
                <cd:Field name="Creator_Version">
                  <cd:Text namexml="Creator_Version">
                    <cd:Description>Version of the tool</cd:Description>
                  </cd:Text>
                </cd:Field>
              </cd:Record>
            </cd:Field>
            <cd:Field name="MPH_Default_Fields">
              <cd:Record namexml="MPH_Default_Fields">
                <cd:Description>Values responsible for populating the main product header</cd:Description>
                <cd:Field name="Software_Ver">
                  <cd:Text namexml="Software_Ver">
                    <cd:Description>Software version number of processing software. Format: name of processor (up to 10 char) version number (4 char)</cd:Description>
                  </cd:Text>
                </cd:Field>
                <cd:Field name="Proc_Centre">
                  <cd:Text namexml="Proc_Centre">
                    <cd:Description>AEOLUS PHS</cd:Description>
                  </cd:Text>
                </cd:Field>
              </cd:Record>
            </cd:Field>
            <cd:Field name="SPH_Default_Fields">
              <cd:Record namexml="SPH_Default_Fields">
                <cd:Description>Values responsible for populating the specific product header</cd:Description>
                <cd:Field name="DOI">
                  <cd:Text namexml="DOI">
                    <cd:Description>Digital object identifier (up to 25 char)</cd:Description>
                  </cd:Text>
                </cd:Field>
              </cd:Record>
            </cd:Field>
            <cd:Field name="WVM_Params">
              <cd:Record namexml="WVM_Params">
                <cd:Description>Processing parameters for Wind Velocity Measurement</cd:Description>
                <cd:Field name="File_Type">
                  <cd:Text namexml="File_Type">
                    <cd:Description>File type string</cd:Description>
                  </cd:Text>
                </cd:Field>
                <cd:Field name="Sph_Descriptor">
                  <cd:Text namexml="Sph_Descriptor">
                    <cd:Description>ASCII string describing the product</cd:Description>
                  </cd:Text>
                </cd:Field>
              </cd:Record>
            </cd:Field>
            <cd:Field name="ISR_Params">
              <cd:Record namexml="ISR_Params">
                <cd:Description>Processing parameters for Instrument Spectral Registration</cd:Description>
                <cd:Field name="File_Type">
                  <cd:Text namexml="File_Type">
                    <cd:Description>File type string</cd:Description>
                  </cd:Text>
                </cd:Field>
                <cd:Field name="Sph_Descriptor">
                  <cd:Text namexml="Sph_Descriptor">
                    <cd:Description>ASCII string describing the product</cd:Description>
                  </cd:Text>
                </cd:Field>
              </cd:Record>
            </cd:Field>
            <cd:Field name="IAT_Params">
              <cd:Record namexml="IAT_Params">
                <cd:Description>Processing parameters for Instrument Auto Test</cd:Description>
                <cd:Field name="File_Type">
                  <cd:Text namexml="File_Type">
                    <cd:Description>File type string</cd:Description>
                  </cd:Text>
                </cd:Field>
                <cd:Field name="Sph_Descriptor">
                  <cd:Text namexml="Sph_Descriptor">
                    <cd:Description>ASCII string describing the product</cd:Description>
                  </cd:Text>
                </cd:Field>
              </cd:Record>
            </cd:Field>
            <cd:Field name="DCC_Params">
              <cd:Record namexml="DCC_Params">
                <cd:Description>Processing parameters for Dark Current Calibration</cd:Description>
                <cd:Field name="File_Type">
                  <cd:Text namexml="File_Type">
                    <cd:Description>File type string</cd:Description>
                  </cd:Text>
                </cd:Field>
                <cd:Field name="Sph_Descriptor">
                  <cd:Text namexml="Sph_Descriptor">
                    <cd:Description>ASCII string describing the product</cd:Description>
                  </cd:Text>
                </cd:Field>
              </cd:Record>
            </cd:Field>
            <cd:Field name="IDC_Params">
              <cd:Record namexml="IDC_Params">
                <cd:Description>Processing parameters for Instrument Defocus Characterisation</cd:Description>
                <cd:Field name="File_Type">
                  <cd:Text namexml="File_Type">
                    <cd:Description>File type string</cd:Description>
                  </cd:Text>
                </cd:Field>
                <cd:Field name="Sph_Descriptor">
                  <cd:Text namexml="Sph_Descriptor">
                    <cd:Description>ASCII string describing the product</cd:Description>
                  </cd:Text>
                </cd:Field>
              </cd:Record>
            </cd:Field>
            <cd:Field name="IRC_Params">
              <cd:Record namexml="IRC_Params">
                <cd:Description>Processing parameters for Instrument Response Calibration</cd:Description>
                <cd:Field name="File_Type">
                  <cd:Text namexml="File_Type">
                    <cd:Description>File type string</cd:Description>
                  </cd:Text>
                </cd:Field>
                <cd:Field name="Sph_Descriptor">
                  <cd:Text namexml="Sph_Descriptor">
                    <cd:Description>ASCII string describing the product</cd:Description>
                  </cd:Text>
                </cd:Field>
              </cd:Record>
            </cd:Field>
            <cd:Field name="OWV_Params">
              <cd:Record namexml="OWV_Params">
                <cd:Description>Processing parameters for Offline Wind Measurement Calibration</cd:Description>
                <cd:Field name="File_Type">
                  <cd:Text namexml="File_Type">
                    <cd:Description>File type string</cd:Description>
                  </cd:Text>
                </cd:Field>
                <cd:Field name="Sph_Descriptor">
                  <cd:Text namexml="Sph_Descriptor">
                    <cd:Description>ASCII string describing the product</cd:Description>
                  </cd:Text>
                </cd:Field>
              </cd:Record>
            </cd:Field>
            <cd:Field name="NOU_Params">
              <cd:Record namexml="NOU_Params">
                <cd:Description>Processing parameters for No operation and User Defined Calibration</cd:Description>
                <cd:Field name="File_Type">
                  <cd:Text namexml="File_Type">
                    <cd:Description>File type string</cd:Description>
                  </cd:Text>
                </cd:Field>
                <cd:Field name="Sph_Descriptor">
                  <cd:Text namexml="Sph_Descriptor">
                    <cd:Description>ASCII string describing the product</cd:Description>
                  </cd:Text>
                </cd:Field>
                <cd:Field name="Dude_Reference_Height_Bin">
                  <cd:Type namexml="Dude_Reference_Height_Bin">
                    <cd:Integer format="ascii">
                      <cd:Description>Altitude reported for this Dude_Reference_Height_Bin has to be below 0.0 m to identify DUDE data.</cd:Description>
                      <cd:NativeType>uint8</cd:NativeType>
                    </cd:Integer>
                  </cd:Type>
                </cd:Field>
                <cd:Field name="Dude_Instrument_Mode">
                  <cd:Type namexml="Dude_Instrument_Mode">
                    <cd:Integer format="ascii">
                      <cd:Description>Instrument mode setting used to identify valid DUDE measurements.</cd:Description>
                      <cd:NativeType>uint8</cd:NativeType>
                    </cd:Integer>
                  </cd:Type>
                </cd:Field>
                <cd:Field name="Dude_DEU_Detection_Mode">
                  <cd:Type namexml="Dude_DEU_Detection_Mode">
                    <cd:Integer format="ascii">
                      <cd:Description>DEU detection mode setting used to identify DUDE measurements.</cd:Description>
                      <cd:NativeType>uint8</cd:NativeType>
                    </cd:Integer>
                  </cd:Type>
                </cd:Field>
                <cd:Field name="Dude_Energy_Threshold">
                  <cd:Type namexml="Dude_Energy_Threshold">
                    <cd:Attribute name="unit">
                      <cd:Optional/>
                      <cd:FixedValue>mJ</cd:FixedValue>
                    </cd:Attribute>
                    <cd:Float format="ascii">
                      <cd:Description>Energy threshold applied to parameter Avg_UV_Energy to distinguish NOU from DUDE measurements.</cd:Description>
                      <cd:Unit>mJ</cd:Unit>
                      <cd:NativeType>double</cd:NativeType>
                    </cd:Float>
                  </cd:Type>
                </cd:Field>
              </cd:Record>
            </cd:Field>
            <cd:Field name="DCMZ_Params">
              <cd:Record namexml="DCMZ_Params">
                <cd:Description>Processing parameters for Dark Current Memory Zone Calibration</cd:Description>
                <cd:Field name="File_Type">
                  <cd:Text namexml="File_Type">
                    <cd:Description>File type string</cd:Description>
                  </cd:Text>
                </cd:Field>
                <cd:Field name="Sph_Descriptor">
                  <cd:Text namexml="Sph_Descriptor">
                    <cd:Description>ASCII string describing the product</cd:Description>
                  </cd:Text>
                </cd:Field>
              </cd:Record>
            </cd:Field>
            <cd:Field name="LBM_Params">
              <cd:Record namexml="LBM_Params">
                <cd:Description>Processing parameters for Laser Beam Monitoring Calibration</cd:Description>
                <cd:Field name="File_Type">
                  <cd:Text namexml="File_Type">
                    <cd:Description>File type string</cd:Description>
                  </cd:Text>
                </cd:Field>
                <cd:Field name="Sph_Descriptor">
                  <cd:Text namexml="Sph_Descriptor">
                    <cd:Description>ASCII string describing the product</cd:Description>
                  </cd:Text>
                </cd:Field>
              </cd:Record>
            </cd:Field>
            <cd:Field name="Data_Quality">
              <cd:Record namexml="Data_Quality">
                <cd:Description>Data Quality Thresholds</cd:Description>
                <cd:Field name="FWHM_Thresh">
                  <cd:Type namexml="FWHM_Thresh">
                    <cd:Attribute name="unit">
                      <cd:Optional/>
                      <cd:FixedValue>%</cd:FixedValue>
                    </cd:Attribute>
                    <cd:Float format="ascii">
                      <cd:Description>Threshold value of internal reference FWHM for quality check</cd:Description>
                      <cd:NativeType>double</cd:NativeType>
                    </cd:Float>
                  </cd:Type>
                </cd:Field>
                <cd:Field name="Freq_Var_From_Peak_Thresh">
                  <cd:Type namexml="Freq_Var_From_Peak_Thresh">
                    <cd:Attribute name="unit">
                      <cd:Optional/>
                      <cd:FixedValue>%</cd:FixedValue>
                    </cd:Attribute>
                    <cd:Float format="ascii">
                      <cd:Description>Threshold value where the variance of frequency from peak (determined by curve fit to the sum of all pulses in the measurement) is out of range</cd:Description>
                      <cd:NativeType>double</cd:NativeType>
                    </cd:Float>
                  </cd:Type>
                </cd:Field>
                <cd:Field name="Measurement_Data_Missing">
                  <cd:Type namexml="Measurement_Data_Missing">
                    <cd:Integer format="ascii">
                      <cd:Description>TRUE= measurement data not present in BRC (for n &gt; number of meansurements in BRC)</cd:Description>
                      <cd:NativeType>uint8</cd:NativeType>
                      <cd:Mapping string="FALSE" value="0"/>
                      <cd:Mapping string="False" value="0"/>
                      <cd:Mapping string="false" value="0"/>
                      <cd:Mapping string="TRUE" value="1"/>
                      <cd:Mapping string="True" value="1"/>
                      <cd:Mapping string="true" value="1"/>
                      <ct:NamedTest id="BooleanXmlTest"/>
                    </cd:Integer>
                  </cd:Type>
                </cd:Field>
                <cd:Field name="Disable_RL_Checks">
                  <cd:Type namexml="Disable_RL_Checks">
                    <cd:Integer format="ascii">
                      <cd:Description>If set to true, the pulse validity check ignores the status of the 'RLH Tuned' and 'RL Locked' flags.</cd:Description>
                      <cd:NativeType>uint8</cd:NativeType>
                      <cd:Mapping string="FALSE" value="0"/>
                      <cd:Mapping string="False" value="0"/>
                      <cd:Mapping string="false" value="0"/>
                      <cd:Mapping string="TRUE" value="1"/>
                      <cd:Mapping string="True" value="1"/>
                      <cd:Mapping string="true" value="1"/>
                      <ct:NamedTest id="BooleanXmlTest"/>
                    </cd:Integer>
                  </cd:Type>
                </cd:Field>
                <cd:Field name="Meas_Cavity_Lock_Status_Thresh">
                  <cd:Type namexml="Meas_Cavity_Lock_Status_Thresh">
                    <cd:Attribute name="unit">
                      <cd:Optional/>
                      <cd:FixedValue>%</cd:FixedValue>
                    </cd:Attribute>
                    <cd:Float format="ascii">
                      <cd:Description>Maximum allowable occurance of cavity lock status for measurement</cd:Description>
                      <cd:NativeType>double</cd:NativeType>
                    </cd:Float>
                  </cd:Type>
                </cd:Field>
                <cd:Field name="Image_Cavity_Lock_Status_Thresh">
                  <cd:Type namexml="Image_Cavity_Lock_Status_Thresh">
                    <cd:Attribute name="unit">
                      <cd:Optional/>
                      <cd:FixedValue>%</cd:FixedValue>
                    </cd:Attribute>
                    <cd:Float format="ascii">
                      <cd:Description>Maximum allowable occurance of cavity lock status for image</cd:Description>
                      <cd:NativeType>double</cd:NativeType>
                    </cd:Float>
                  </cd:Type>
                </cd:Field>
              </cd:Record>
            </cd:Field>
            <cd:Field name="Transfer_Curve_Selection">
              <cd:Record namexml="Transfer_Curve_Selection">
                <cd:Field name="DEU">
                  <cd:Text namexml="DEU">
                    <cd:Description>DEU unit selection; possible values: "A" or "B"</cd:Description>
                  </cd:Text>
                </cd:Field>
                <cd:Field name="TLE">
                  <cd:Text namexml="TLE">
                    <cd:Description>TLE unit selection; possible values: "1" or "2"</cd:Description>
                  </cd:Text>
                </cd:Field>
                <cd:Field name="ACDM">
                  <cd:Text namexml="ACDM">
                    <cd:Description>ACDM unit selection; possible values: "A" or "B"</cd:Description>
                  </cd:Text>
                </cd:Field>
              </cd:Record>
            </cd:Field>
            <cd:Field name="Perform_Satellite_Velocity_Correction">
              <cd:Type namexml="Perform_Satellite_Velocity_Correction">
                <cd:Integer format="ascii">
                  <cd:Description>TRUE= measurement data not present in BRC (for n &gt; number of meansurements in BRC)</cd:Description>
                  <cd:NativeType>uint8</cd:NativeType>
                  <cd:Mapping string="FALSE" value="0"/>
                  <cd:Mapping string="False" value="0"/>
                  <cd:Mapping string="false" value="0"/>
                  <cd:Mapping string="TRUE" value="1"/>
                  <cd:Mapping string="True" value="1"/>
                  <cd:Mapping string="true" value="1"/>
                  <ct:NamedTest id="BooleanXmlTest"/>
                </cd:Integer>
              </cd:Type>
            </cd:Field>
            <cd:Field name="Min_Num_BRC">
              <cd:Type namexml="Min_Num_BRC">
                <cd:Integer format="ascii">
                  <cd:Description>If number of output BRCs is below this threshold, no output product will be written. This applies to all modes.</cd:Description>
                  <cd:NativeType>uint8</cd:NativeType>
                </cd:Integer>
              </cd:Type>
            </cd:Field>
          </cd:Record>
          <ct:NamedTest id="ValueOfCountAttributeForParent"/>
        </cd:Array>
      </cd:Field>
    </cd:Record>
  </cd:Field>
</cd:Record>
