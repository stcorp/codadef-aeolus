    <cd:Float>
      <cd:Description>Fixed fraction of Lorentzian function of Voigt used in the Mie Core 3 for atmospheric signals</cd:Description>
      <cd:BitSize>64</cd:BitSize>
      <cd:NativeType>double</cd:NativeType>
      <ct:NamedTest id="FloatIsFinite"/>
    </cd:Float>
  </cd:Field>
  <cd:Field name="lorentzian_fration_of_voigt_reference_pulse">
    <cd:Float>
      <cd:Description>Fixed fraction of Lorentzian function of Voigt used in the Mie Core 3 for the internal reference</cd:Description>
      <cd:BitSize>64</cd:BitSize>
      <cd:NativeType>double</cd:NativeType>
      <ct:NamedTest id="FloatIsFinite"/>
    </cd:Float>
  </cd:Field>
