    <cd:Float>
      <cd:Description>Fixed Voigt function FWHM used in the Mie Core 3 for the internal reference</cd:Description>
      <cd:Unit>ACCD pixel</cd:Unit>
      <cd:BitSize>64</cd:BitSize>
      <cd:NativeType>double</cd:NativeType>
      <ct:NamedTest id="FloatIsFinite"/>
    </cd:Float>
  </cd:Field>
