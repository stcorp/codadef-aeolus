    <cd:Integer>
      <cd:Description>Check if pixels with the two highest most signals areneighbours</cd:Description>
      <cd:BitSize>8</cd:BitSize>
      <cd:NativeType>uint8</cd:NativeType>
      <ct:NamedTest id="BooleanBinTest"/>
