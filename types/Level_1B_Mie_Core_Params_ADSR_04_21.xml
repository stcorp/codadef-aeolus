      <cd:Description>Check mean signal of lowest 12 against highestsignal in ACCD row</cd:Description>
      <cd:BitSize>8</cd:BitSize>
      <cd:NativeType>uint8</cd:NativeType>
      <ct:NamedTest id="BooleanBinTest"/>
    </cd:Integer>
