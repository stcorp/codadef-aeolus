    <cd:Integer>
      <cd:Description>Check if pixels with the three highest signals are neighbours</cd:Description>
      <cd:BitSize>8</cd:BitSize>
      <cd:NativeType>uint8</cd:NativeType>
      <ct:NamedTest id="BooleanBinTest"/>
