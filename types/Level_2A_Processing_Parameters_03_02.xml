<?xml version="1.0"?>                                                                          
<cd:Record name="Level_2A_Processing_Parameters_03_02" format="xml" namexml="Level_2A_Processing_Parameters" last-modified="2015-12-09" xmlns:cd="http://www.stcorp.nl/coda/definition/2008/07" xmlns:ct="http://www.stcorp.nl/coda/test/2008/10">
  <cd:Field name="List_of_Data_Set_Records">
    <cd:Record namexml="List_of_Data_Set_Records">
      <cd:Description>List of Data Set Records</cd:Description>
      <cd:Attribute name="count"/>
      <cd:Field name="Data_Set_Record">
        <cd:Array>
          <cd:Dimension/>
          <cd:Record namexml="Data_Set_Record">
            <cd:Description>Data Set Record</cd:Description>                 
            <cd:Field name="FH_Default_Fields">
              <cd:Record namexml="FH_Default_Fields">
                <cd:Description>Fields responsible for populating the Fixed Header</cd:Description>
                <cd:Field name="File_Description">
                  <cd:Text namexml="File_Description">
                    <cd:Description>1-line description of the file</cd:Description>
                  </cd:Text>
                </cd:Field>
                <cd:Field name="Notes">
                  <cd:Text namexml="Notes">
                    <cd:Description>Multi-lines free text</cd:Description>
                  </cd:Text>
                </cd:Field>
                <cd:Field name="Mission">
                  <cd:Text namexml="Mission">
                    <cd:Description>Aeolus</cd:Description>
                  </cd:Text>
                </cd:Field>
                <cd:Field name="Mission_Id">
                  <cd:Text namexml="Mission_Id">
                    <cd:Description>AE</cd:Description>
                  </cd:Text>
                </cd:Field>
                <cd:Field name="File_Class">
                  <cd:Text namexml="File_Class">
                    <cd:Description>1-Line description of the file class</cd:Description>
                  </cd:Text>
                </cd:Field>
                <cd:Field name="File_Version">
                  <cd:Type namexml="File_Version">
                    <cd:Integer format="ascii">
                      <cd:Description>4 digits used to distinguish between versions of a file having the same validity period</cd:Description>
                      <cd:NativeType>int16</cd:NativeType>
                    </cd:Integer>
                  </cd:Type>
                </cd:Field>
                <cd:Field name="System">
                  <cd:Text namexml="System">
                    <cd:Description>Aeolus L1bP</cd:Description>
                  </cd:Text>
                </cd:Field>
                <cd:Field name="Creator">
                  <cd:Text namexml="Creator">
                    <cd:Description>Aeolus L1bP</cd:Description>
                  </cd:Text>
                </cd:Field>
                <cd:Field name="Creator_Version">
                  <cd:Text namexml="Creator_Version">
                    <cd:Description>Version of the tool</cd:Description>
                  </cd:Text>
                </cd:Field>
              </cd:Record>
            </cd:Field>
            <cd:Field name="MPH_Default_Fields">
              <cd:Record namexml="MPH_Default_Fields">
                <cd:Description>Values responsible for populating the main product header</cd:Description>
                <cd:Field name="Ref_Doc">
                  <cd:Text namexml="Ref_Doc">
                    <cd:Description>Reference document describing the product</cd:Description>
                  </cd:Text>
                </cd:Field>
                <cd:Field name="Software_Ver">
                  <cd:Text namexml="Software_Ver">
                    <cd:Description>Software version number of processing software. Format: name of processor (up to 10 char) version number (4 char)</cd:Description>
                  </cd:Text>
                </cd:Field>
                <cd:Field name="Proc_Centre">
                  <cd:Text namexml="Proc_Centre">
                    <cd:Description>AEOLUS PHS</cd:Description>
                  </cd:Text>
                </cd:Field>
              </cd:Record>
            </cd:Field>
            <cd:Field name="Config_Params">
              <cd:Record namexml="Config_Params">
                <cd:Field name="SAMS">
                  <cd:Record namexml="SAMS">
                    <cd:Field name="Wavelength">
                      <cd:Type namexml="Wavelength">
                        <cd:Attribute name="unit"/>
                        <cd:Float format="ascii">
                          <cd:Description>Laser wavelength.</cd:Description>
                          <cd:Unit>m</cd:Unit>
                          <cd:NativeType>double</cd:NativeType>
                        </cd:Float>
                      </cd:Type>
                    </cd:Field>
                    <cd:Field name="Maximum_Altitude">
                      <cd:Type namexml="Maximum_Altitude">
                        <cd:Attribute name="unit"/>
                        <cd:Integer format="ascii">
                          <cd:Description>Altitude up to which AMD profiles are calculated.</cd:Description>
                          <cd:Unit>m</cd:Unit>
                          <cd:NativeType>uint16</cd:NativeType>
                        </cd:Integer>
                      </cd:Type>
                    </cd:Field>
                    <cd:Field name="Minimum_Altitude">
                      <cd:Type namexml="Minimum_Altitude">
                        <cd:Attribute name="unit"/>
                        <cd:Integer format="ascii">
                          <cd:Description>Altitude down to which AMD profiles are calculated.</cd:Description>
                          <cd:Unit>m</cd:Unit>
                          <cd:NativeType>uint16</cd:NativeType>
                        </cd:Integer>
                      </cd:Type>
                    </cd:Field>
                    <cd:Field name="Altitude_Step">
                      <cd:Type namexml="Altitude_Step">
                        <cd:Attribute name="unit"/>
                        <cd:Integer format="ascii">
                          <cd:Description>Vertical resolution of the calculation of AMD profiles.</cd:Description>
                          <cd:Unit>m</cd:Unit>
                          <cd:NativeType>uint16</cd:NativeType>
                        </cd:Integer>
                      </cd:Type>
                    </cd:Field>
                    <cd:Field name="Betam_Factor">
                      <cd:Type namexml="Betam_Factor">
                        <cd:Float format="ascii">
                          <cd:Description>Multiplicative factor in the formula to get the molecular backscatter coefficient from atmospheric data.</cd:Description>
                          <cd:NativeType>double</cd:NativeType>
                        </cd:Float>
                      </cd:Type>
                    </cd:Field>
                    <cd:Field name="Reference_Wavelength">
                      <cd:Type namexml="Reference_Wavelength">
                        <cd:Attribute name="unit"/>
                        <cd:Float format="ascii">
                          <cd:Description>Reference wavelength in the formula to get the molecular backscatter coefficient from atmospheric data.</cd:Description>
                          <cd:Unit>m</cd:Unit>
                          <cd:NativeType>double</cd:NativeType>
                        </cd:Float>
                      </cd:Type>
                    </cd:Field>
                    <cd:Field name="Wavelength_Exponent">
                      <cd:Type namexml="Wavelength_Exponent">
                        <cd:Float format="ascii">
                          <cd:Description>Exponent of the ratio of wavelengths in the formula to get the molecular backscatter coefficient from atmospheric data.</cd:Description>
                          <cd:NativeType>double</cd:NativeType>
                        </cd:Float>
                      </cd:Type>
                    </cd:Field>
                    <cd:Field name="Reference_Pressure">
                      <cd:Type namexml="Reference_Pressure">
                        <cd:Attribute name="unit"/>
                        <cd:Integer format="ascii">
                          <cd:Description>Reference pressure in the formula to get the molecular backscatter coefficient from atmospheric data.</cd:Description>
                          <cd:Unit>hPa</cd:Unit>
                          <cd:NativeType>uint16</cd:NativeType>
                        </cd:Integer>
                      </cd:Type>
                    </cd:Field>
                    <cd:Field name="Reference_Temperature">
                      <cd:Type namexml="Reference_Temperature">
                        <cd:Attribute name="unit"/>
                        <cd:Integer format="ascii">
                          <cd:Description>Reference temperature in the formula to get the molecular backscatter coefficient from atmospheric data</cd:Description>
                          <cd:Unit>K</cd:Unit>
                          <cd:NativeType>uint16</cd:NativeType>
                        </cd:Integer>
                      </cd:Type>
                    </cd:Field>
                    <cd:Field name="MatchAMD">
                      <cd:Record namexml="MatchAMD">
                        <cd:Field name="Earth_Radius">
                          <cd:Type namexml="Earth_Radius">
                            <cd:Attribute name="unit"/>
                            <cd:Integer format="ascii">
                              <cd:Description>Radius of the blue planet.</cd:Description>
                              <cd:Unit>m</cd:Unit>
                              <cd:NativeType>uint32</cd:NativeType>
                            </cd:Integer>
                          </cd:Type>
                        </cd:Field>
                        <cd:Field name="Time_Gap_Threshold">
                          <cd:Type namexml="Time_Gap_Threshold">
                            <cd:Attribute name="unit"/>
                            <cd:Integer format="ascii">
                              <cd:Description>Threshold lesser than which the delay between a measurement and an AMD profile must be.</cd:Description>
                              <cd:Unit>s</cd:Unit>
                              <cd:NativeType>uint16</cd:NativeType>
                            </cd:Integer>
                          </cd:Type>
                        </cd:Field>
                      </cd:Record>
                    </cd:Field>
                  </cd:Record>
                </cd:Field>
                <cd:Field name="L2Ap">
                  <cd:Record namexml="L2Ap">
                    <cd:Field name="Quality_Flag">
                      <cd:Record namexml="Quality_Flag">
                        <cd:Field name="Mie_Snr_Threshold">
                          <cd:Type namexml="Mie_Snr_Threshold">
                            <cd:Integer format="ascii">
                              <cd:Description>Threshold on the Mie SNR to consider the Mie signal as valid</cd:Description>
                              <cd:NativeType>uint16</cd:NativeType>
                            </cd:Integer>
                          </cd:Type>
                        </cd:Field>
                        <cd:Field name="Rayleigh_Snr_Threshold">
                          <cd:Type namexml="Rayleigh_Snr_Threshold">
                            <cd:Integer format="ascii">
                              <cd:Description>Threshold on the Rayleigh SNR to consider the Rayleigh signal as valid</cd:Description>
                              <cd:NativeType>uint16</cd:NativeType>
                            </cd:Integer>
                          </cd:Type>
                        </cd:Field>
                        <cd:Field name="Mid_Mie_Snr_Threshold">
                          <cd:Type namexml="Mid_Mie_Snr_Threshold">
                            <cd:Integer format="ascii">
                              <cd:Description>Threshold on the Mie SNR defined on middle bins to consider the Mie signal as valid</cd:Description>
                              <cd:NativeType>uint16</cd:NativeType>
                            </cd:Integer>
                          </cd:Type>
                        </cd:Field>
                        <cd:Field name="Mid_Rayleigh_Snr_Threshold">
                          <cd:Type namexml="Mid_Rayleigh_Snr_Threshold">
                            <cd:Integer format="ascii">
                              <cd:Description>Threshold on the Rayleigh SNR defined on middle bins to consider the Rayleigh signal as valid</cd:Description>
                              <cd:NativeType>uint16</cd:NativeType>
                            </cd:Integer>
                          </cd:Type>
                        </cd:Field>
                        <cd:Field name="Beta_Error_Bar_Threshold">
                          <cd:Type namexml="Beta_Error_Bar_Threshold">
                            <cd:Attribute name="unit"/>
                            <cd:Integer format="ascii">
                              <cd:Description>Threshold on the relative error of backscatter retrieval (ratio of the variance of the backscatter coefficient to the backscatter coefficient)</cd:Description>
                              <cd:Unit>%</cd:Unit>
                              <cd:NativeType>uint16</cd:NativeType>
                            </cd:Integer>
                          </cd:Type>
                        </cd:Field>
                        <cd:Field name="Alpha_Error_Bar_Threshold">
                          <cd:Type namexml="Alpha_Error_Bar_Threshold">
                            <cd:Attribute name="unit"/>
                            <cd:Integer format="ascii">
                              <cd:Description>Threshold on the relative error of extinction retrieval (ratio of the variance of the extinction coefficient to the extinction coefficient)</cd:Description>
                              <cd:Unit>%</cd:Unit>
                              <cd:NativeType>uint16</cd:NativeType>
                            </cd:Integer>
                          </cd:Type>
                        </cd:Field>
                        <cd:Field name="Mid_Beta_Error_Bar_Threshold">
                          <cd:Type namexml="Mid_Beta_Error_Bar_Threshold">
                            <cd:Attribute name="unit"/>
                            <cd:Integer format="ascii">
                              <cd:Description>Threshold on the relative error of backscatter retrieval on middle bins (ratio of the variance of the backscatter coefficient to the backscatter coefficient defined on middle bins)</cd:Description>
                              <cd:Unit>%</cd:Unit>
                              <cd:NativeType>uint16</cd:NativeType>
                            </cd:Integer>
                          </cd:Type>
                        </cd:Field>
                        <cd:Field name="Mid_Alpha_Error_Bar_Threshold">
                          <cd:Type namexml="Mid_Alpha_Error_Bar_Threshold">
                            <cd:Attribute name="unit"/>
                            <cd:Integer format="ascii">
                              <cd:Description>Threshold on the relative error of extinction retrieval on middle bins (ratio of the variance of the extinction coefficient to the extinction coefficient defined on middle bins)</cd:Description>
                              <cd:Unit>%</cd:Unit>
                              <cd:NativeType>uint16</cd:NativeType>
                            </cd:Integer>
                          </cd:Type>
                        </cd:Field>
                        <cd:Field name="Total_Optical_Depth_Threshold">
                          <cd:Type namexml="Total_Optical_Depth_Threshold">
                            <cd:Integer format="ascii">
                              <cd:Description>Threshold above which we consider that the backscattered signal was totally attenuated by absorption.</cd:Description>
                              <cd:NativeType>uint16</cd:NativeType>
                            </cd:Integer>
                          </cd:Type>
                        </cd:Field>
                        <cd:Field name="BER_Max_Threshold">
                          <cd:Type namexml="BER_Max_Threshold">
                            <cd:Attribute name="unit"/>
                            <cd:Float format="ascii">
                              <cd:Description>Above this value, the BER retrieval is supposed to be invalid</cd:Description>
                              <cd:Unit>sr^-1</cd:Unit>
                              <cd:NativeType>double</cd:NativeType>
                            </cd:Float>
                          </cd:Type>
                        </cd:Field>
                        <cd:Field name="BER_Min_Threshold">
                          <cd:Type namexml="BER_Min_Threshold">
                            <cd:Attribute name="unit"/>
                            <cd:Float format="ascii">
                              <cd:Description>Below this value, the BER retrieval is supposed to be invalid</cd:Description>
                              <cd:Unit>sr^-1</cd:Unit>
                              <cd:NativeType>double</cd:NativeType>
                            </cd:Float>
                          </cd:Type>
                        </cd:Field>
                      </cd:Record>
                    </cd:Field>
                    <cd:Field name="Find_Matching_Bins">
                      <cd:Record namexml="Find_Matching_Bins">
                        <cd:Field name="Altitude_Match_Threshold">
                          <cd:Type namexml="Altitude_Match_Threshold">
                            <cd:Attribute name="unit"/>
                            <cd:Integer format="ascii">
                              <cd:Description>Threshold lesser than which the gap between a Rayleigh bin border and a Mie bin border must be for them to match.</cd:Description>
                              <cd:Unit>m</cd:Unit>
                              <cd:NativeType>uint16</cd:NativeType>
                            </cd:Integer>
                          </cd:Type>
                        </cd:Field>
                      </cd:Record>
                    </cd:Field>
                    <cd:Field name="FeatureFinder">
                      <cd:Record namexml="FeatureFinder">
                        <cd:Field name="Group_Energy_Threshold">
                          <cd:Type namexml="Group_Energy_Threshold">
                            <cd:Attribute name="unit"/>
                            <cd:Float format="ascii">
                              <cd:Description>Parameter used to tune the minimum size of groups before they may be nibbled.</cd:Description>
                              <cd:Unit>J</cd:Unit>
                              <cd:NativeType>double</cd:NativeType>
                            </cd:Float>
                          </cd:Type>
                        </cd:Field>
                        <cd:Field name="Gaussian_Mask_Std5">
                          <cd:Type namexml="Gaussian_Mask_Std5">
                            <cd:Float format="ascii">
                              <cd:Description>Standard deviation of the smoothing Gaussian mask when it is 5-bin wide, i.e. when the minimum group length is 3.</cd:Description>
                              <cd:NativeType>double</cd:NativeType>
                            </cd:Float>
                          </cd:Type>
                        </cd:Field>
                        <cd:Field name="Gaussian_Mask_Std3">
                          <cd:Type namexml="Gaussian_Mask_Std3">
                            <cd:Float format="ascii">
                              <cd:Description>Standard deviation of the smoothing Gaussian mask when it is 3-bin wide, i.e. when the minimum group length is 2.</cd:Description>
                              <cd:NativeType>double</cd:NativeType>
                            </cd:Float>
                          </cd:Type>
                        </cd:Field>
                        <cd:Field name="Emptiness_Betap_Threshold">
                          <cd:Type namexml="Emptiness_Betap_Threshold">
                            <cd:Attribute name="unit"/>
                            <cd:Float format="ascii">
                              <cd:Description>Value below which it is considered that there is no significant amount of particles.</cd:Description>
                              <cd:Unit>m^-1 sr^-1</cd:Unit>
                              <cd:NativeType>double</cd:NativeType>
                            </cd:Float>
                          </cd:Type>
                        </cd:Field>
                        <cd:Field name="Negative_Betap_Threshold">
                          <cd:Type namexml="Negative_Betap_Threshold">
                            <cd:Attribute name="unit"/>
                            <cd:Float format="ascii">
                              <cd:Description>Negative value under which fluctuations are considered to be too high, leading to a rejection of the bin.</cd:Description>
                              <cd:Unit>m^-1 sr^-1</cd:Unit>
                              <cd:NativeType>double</cd:NativeType>
                            </cd:Float>
                          </cd:Type>
                        </cd:Field>
                        <cd:Field name="Nibbling_Threshold">
                          <cd:Type namexml="Nibbling_Threshold">
                            <cd:Attribute name="unit"/>
                            <cd:Float format="ascii">
                              <cd:Description>Threshold on the normalised variation of the standard deviation of the smoothed particulate backscatter when attempting to swap a bin between two neightbour groups.</cd:Description>
                              <cd:Unit>m^-1 sr^-1</cd:Unit>
                              <cd:NativeType>double</cd:NativeType>
                            </cd:Float>
                          </cd:Type>
                        </cd:Field>
                        <cd:Field name="Remove_Group_Relative_Error_Threshold">
                          <cd:Type namexml="Remove_Group_Relative_Error_Threshold">
                            <cd:Float format="ascii">
                              <cd:Description>Threshold on the unsmoothed particulate backscatter relative error, esti ated by the error propagation, above which products from a group will be considered untrustworthy and rejected.</cd:Description>
                              <cd:NativeType>double</cd:NativeType>
                            </cd:Float>
                          </cd:Type>
                        </cd:Field>
                        <cd:Field name="List_of_Smoothed_Betap_Limits">
                          <cd:Record namexml="List_of_Smoothed_Betap_Limits">
                            <cd:Attribute name="count"/>
                            <cd:Field name="Smoothed_Betap_Limit">
                              <cd:Array>
                                <cd:Dimension/>
                                <cd:Record namexml="Smoothed_Betap_Limit">
                                  <cd:Field name="BetapStd">
                                    <cd:Type namexml="BetapStd">
                                      <cd:Attribute name="unit"/>
                                      <cd:Float format="ascii">
                                        <cd:Description>Smoothed particulate backscatter standard deviation.</cd:Description>
                                        <cd:Unit>m^-1 sr^-1</cd:Unit>
                                        <cd:NativeType>double</cd:NativeType>
                                      </cd:Float>
                                    </cd:Type>
                                  </cd:Field>
                                  <cd:Field name="BetapMean">
                                    <cd:Type namexml="BetapMean">
                                      <cd:Attribute name="unit"/>
                                      <cd:Float format="ascii">
                                        <cd:Description>Corresponding smoothed particulate backscatter mean.</cd:Description>
                                        <cd:Unit>m^-1 sr^-1</cd:Unit>
                                        <cd:NativeType>double</cd:NativeType>
                                      </cd:Float>
                                    </cd:Type>
                                  </cd:Field>
                                </cd:Record>
                              </cd:Array>
                            </cd:Field>
                          </cd:Record>
                        </cd:Field>
                        <cd:Field name="DSigma_Coefficient">
                          <cd:Type namexml="DSigma_Coefficient">
                            <cd:Float format="ascii">
                              <cd:Description>Coefficient to infer a limit to the smoothed particulate backscatter standard deviation variation when adding a bin to a group, making use of the Smoothed_Betap_Limits distribution above.</cd:Description>
                              <cd:NativeType>double</cd:NativeType>
                            </cd:Float>
                          </cd:Type>
                        </cd:Field>
                      </cd:Record>
                    </cd:Field>
                    <cd:Field name="SCA">
                      <cd:Record namexml="SCA">
                        <cd:Field name="Clear_FMB_Beta_Threshold">
                          <cd:Type namexml="Clear_FMB_Beta_Threshold">
                            <cd:Attribute name="unit"/>
                            <cd:Float format="ascii">
                              <cd:Description>Particulate backscatter value below which the first matching bin will be considered empty of any particle, introducing thus no oscillating bias in the extinction retrieval.</cd:Description>
                              <cd:Unit>m^-1 sr^-1</cd:Unit>
                              <cd:NativeType>double</cd:NativeType>
                            </cd:Float>
                          </cd:Type>
                        </cd:Field>
                        <cd:Field name="Negative_Betap_Threshold">
                          <cd:Type namexml="Negative_Betap_Threshold">
                            <cd:Attribute name="unit"/>
                            <cd:Float format="ascii">
                              <cd:Description>Particulate backscatter value below which the coefficient and its derivatives will be considered invalid and set to -1. Slightly ngative values may be accepted to catch a glimps of the oscillations around zero.</cd:Description>
                              <cd:Unit>m^-1 sr^-1</cd:Unit>
                              <cd:NativeType>double</cd:NativeType>
                            </cd:Float>
                          </cd:Type>
                        </cd:Field>
                        <cd:Field name="Negative_SLOD_Threshold">
                          <cd:Type namexml="Negative_SLOD_Threshold">
                            <cd:Float format="ascii">
                              <cd:Description>SLOD value below which the coefficient and its derivatives will be considered invalid and set to -1. Slightly ngative values may be accepted to catch a glimps of the oscillations around zero.</cd:Description>
                              <cd:NativeType>double</cd:NativeType>
                            </cd:Float>
                          </cd:Type>
                        </cd:Field>
                        <cd:Field name="Negative_Betap_Threshold2">
                          <cd:Type namexml="Negative_Betap_Threshold2">
                            <cd:Attribute name="unit"/>
                            <cd:Float format="ascii">
                              <cd:Description>Particulate backscatter value below which thecoefficient and its derivatives will be considered invalid and set to - 1. Slightly ngative values may be accepted to catch a glimps of the oscillations around zero.</cd:Description>
                              <cd:Unit>m^-1 sr^-1</cd:Unit>
                              <cd:NativeType>double</cd:NativeType>
                            </cd:Float>
                          </cd:Type>
                        </cd:Field>
                      </cd:Record>
                    </cd:Field>
                    <cd:Field name="ICA">
                      <cd:Record namexml="ICA">
                        <cd:Field name="Emptiness_Betap_Threshold">
                          <cd:Type namexml="Emptiness_Betap_Threshold">
                            <cd:Attribute name="unit"/>
                            <cd:Float format="ascii">
                              <cd:Description>Particulate backscatter value below which it is considered that there is no significant amount of particles. Note that it may be the same as for the feature finder.</cd:Description>
                              <cd:Unit>m^-1 sr^-1</cd:Unit>
                              <cd:NativeType>double</cd:NativeType>
                            </cd:Float>
                          </cd:Type>
                        </cd:Field>
                        <cd:Field name="Negative_Betap_Threshold">
                          <cd:Type namexml="Negative_Betap_Threshold">
                            <cd:Attribute name="unit"/>
                            <cd:Float format="ascii">
                              <cd:Description>Negative value under which fluctuations are considered to be too high, leading to a rejection of the bin.</cd:Description>
                              <cd:Unit>m^-1 sr^-1</cd:Unit>
                              <cd:NativeType>double</cd:NativeType>
                            </cd:Float>
                          </cd:Type>
                        </cd:Field>
                        <cd:Field name="List_of_Filling_Cases">
                          <cd:Record namexml="List_of_Filling_Cases">
                            <cd:Attribute name="count"/>
                            <cd:Field name="Filling_Case">
                              <cd:Array>
                                <cd:Dimension/>
                                <cd:Record namexml="Filling_Case">
                                  <cd:Field name="F1">
                                    <cd:Type namexml="F1">
                                      <cd:Float format="ascii">
                                        <cd:Description>Factor of the constant member.</cd:Description>
                                        <cd:NativeType>double</cd:NativeType>
                                      </cd:Float>
                                    </cd:Type>
                                  </cd:Field>
                                  <cd:Field name="F2">
                                    <cd:Type namexml="F2">
                                      <cd:Float format="ascii">
                                        <cd:Description>Factor of the H function.</cd:Description>
                                        <cd:NativeType>double</cd:NativeType>
                                      </cd:Float>
                                    </cd:Type>
                                  </cd:Field>
                                  <cd:Field name="F3">
                                    <cd:Type namexml="F3">
                                      <cd:Float format="ascii">
                                        <cd:Description>Factor of the exponential function.</cd:Description>
                                        <cd:NativeType>double</cd:NativeType>
                                      </cd:Float>
                                    </cd:Type>
                                  </cd:Field>
                                </cd:Record>
                              </cd:Array>
                            </cd:Field>
                          </cd:Record>
                        </cd:Field>
                        <cd:Field name="Credibility_Criterion">
                          <cd:Type namexml="Credibility_Criterion">
                            <cd:Float format="ascii">
                              <cd:Description>Particulate backscatter value below which thecoefficient and its derivatives will be considered invalid and set to - 1. Slightly ngative values may be accepted to catch a glimps of the oscillations around zero.</cd:Description>
                              <cd:NativeType>double</cd:NativeType>
                            </cd:Float>
                          </cd:Type>
                        </cd:Field>
                      </cd:Record>
                    </cd:Field>
                    <cd:Field name="Trans2LOD">
                      <cd:Record namexml="Trans2LOD">
                        <cd:Field name="Iteration_Max">
                          <cd:Type namexml="Iteration_Max">
                            <cd:Integer format="ascii">
                              <cd:Description>Maximum number of iterations in the iterative estimation of the SLOD.</cd:Description>
                              <cd:NativeType>uint16</cd:NativeType>
                            </cd:Integer>
                          </cd:Type>
                        </cd:Field>
                        <cd:Field name="Inversion_Tolerance">
                          <cd:Type namexml="Inversion_Tolerance">
                            <cd:Float format="ascii">
                              <cd:Description>Maximum gap between the ratio of the normalized two way transmission and the squared transmission from the satellite to the bin and the value resulting of the SLOD estimation.</cd:Description>
                              <cd:NativeType>double</cd:NativeType>
                            </cd:Float>
                          </cd:Type>
                        </cd:Field>
                        <cd:Field name="Asymptotic_Expansion_Limit_Coefficient">
                          <cd:Type namexml="Asymptotic_Expansion_Limit_Coefficient">
                            <cd:Float format="ascii">
