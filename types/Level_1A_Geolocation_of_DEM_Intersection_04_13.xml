<?xml version="1.0"?>
<cd:Record xmlns:cd="http://www.stcorp.nl/coda/definition/2008/07" xmlns:ct="http://www.stcorp.nl/coda/test/2008/10" format="binary" name="Level_1A_Geolocation_of_DEM_Intersection_04_13" last-modified="2020-07-15">
  <cd:Field name="latitude_of_dem_intersection">
    <cd:Integer>
      <cd:Description>Latitude of the intersection of DEM and the line-of-sight</cd:Description>
      <cd:Unit>1e-6 degrees_north</cd:Unit>
      <cd:Conversion numerator="1.000000" denominator="1000000.000000">
        <cd:Unit>degrees_north</cd:Unit>
      </cd:Conversion>
      <cd:BitSize>32</cd:BitSize>
      <cd:NativeType>int32</cd:NativeType>
    </cd:Integer>
  </cd:Field>
  <cd:Field name="longitude_of_dem_intersection">
    <cd:Integer>
      <cd:Description>Longitude of the intersection of DEM and the line-of-sight</cd:Description>
      <cd:Unit>1e-6 degrees_east</cd:Unit>
      <cd:Conversion numerator="1.000000" denominator="1000000.000000">
        <cd:Unit>degrees_east</cd:Unit>
      </cd:Conversion>
      <cd:BitSize>32</cd:BitSize>
      <cd:NativeType>int32</cd:NativeType>
    </cd:Integer>
  </cd:Field>
  <cd:Field name="altitude_of_dem_intersection">
    <cd:Float>
      <cd:Description>Altitude of the intersection of DEM and the line-of-sight</cd:Description>
      <cd:Unit>m</cd:Unit>
      <cd:BitSize>64</cd:BitSize>
      <cd:NativeType>double</cd:NativeType>
      <ct:NamedTest id="FloatIsFinite"/>
    </cd:Float>
  </cd:Field>
  <cd:Field name="argument_of_latitude_of_dem_intersection">
    <cd:Integer>
      <cd:Description>Argument of Latitude of the intersection of DEM and the line-of-sight</cd:Description>
      <cd:Unit>1e-6 degrees_north</cd:Unit>
      <cd:Conversion numerator="1.000000" denominator="1000000.000000">
        <cd:Unit>degrees_north</cd:Unit>
      </cd:Conversion>
      <cd:BitSize>32</cd:BitSize>
      <cd:NativeType>int32</cd:NativeType>
    </cd:Integer>
  </cd:Field>
  <cd:Field name="sun_elevation_at_dem_intersection">
    <cd:Float>
      <cd:Description>Sun elevation angle at DEM intersection</cd:Description>
      <cd:Unit>deg</cd:Unit>
      <cd:BitSize>64</cd:BitSize>
      <cd:NativeType>double</cd:NativeType>
      <ct:NamedTest id="FloatIsFinite"/>
    </cd:Float>
  </cd:Field>
</cd:Record>
