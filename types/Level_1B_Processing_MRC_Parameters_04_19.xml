      <cd:Attribute name="unit">
        <cd:Optional/>
        <cd:FixedValue>ACCD pixel</cd:FixedValue>
      </cd:Attribute>
      <cd:Float format="ascii">
        <cd:Description>If MieCore2 is used and the retrieved FWHM of the ground return signal is above this threshold, the ground detection is flagged invalid.</cd:Description>
        <cd:NativeType>double</cd:NativeType>
      </cd:Float>
    </cd:Type>
  </cd:Field>
  <cd:Field name="Mie_Ideal_Calibration_Values">
    <cd:Record>
      <cd:Description>Ideal slope and intercept values used for RDB correction.</cd:Description>
      <cd:Field name="Mie_Internal_Slope">
        <cd:Type namexml="Mie_Internal_Slope">
          <cd:Float format="ascii">
            <cd:Description>Ideal internal reference pulse mean sensitivity (slope of the best straight line of channel response)</cd:Description>
            <cd:NativeType>double</cd:NativeType>
          </cd:Float>
        </cd:Type>
      </cd:Field>
      <cd:Field name="Mie_Internal_Offset">
        <cd:Type namexml="Mie_Internal_Offset">
          <cd:Float format="ascii">
            <cd:Description>Ideal internal reference pulse zero frequency response (y-intercept of the best straight line of channel response)</cd:Description>
            <cd:NativeType>double</cd:NativeType>
          </cd:Float>
        </cd:Type>
      </cd:Field>
      <cd:Field name="Mie_Ground_Slope">
        <cd:Type namexml="Mie_Ground_Slope">
          <cd:Float format="ascii">
            <cd:Description>Ideal ground measurement mean sensitivity (slope of the best straight line of channel response)</cd:Description>
            <cd:NativeType>double</cd:NativeType>
          </cd:Float>
        </cd:Type>
      </cd:Field>
      <cd:Field name="Mie_Ground_Offset">
        <cd:Type namexml="Mie_Ground_Offset">
          <cd:Float format="ascii">
            <cd:Description>Ideal ground measurement zero frequency response (y-intercept of the best straight line of channel response)</cd:Description>
            <cd:NativeType>double</cd:NativeType>
          </cd:Float>
        </cd:Type>
      </cd:Field>
    </cd:Record>
  </cd:Field>
  <cd:Field name="Use_Weighted_Linear_Fit">
    <cd:Type namexml="Use_Weighted_Linear_Fit">
      <cd:Integer format="ascii">
        <cd:Description>Flag TRUE/FALSE to indicate if a weighted or unweighted linear least squares fit is used to compute the mean slope and intercept of the Mie response</cd:Description>
        <cd:NativeType>uint8</cd:NativeType>
        <cd:Mapping string="FALSE" value="0"/>
        <cd:Mapping string="False" value="0"/>
        <cd:Mapping string="false" value="0"/>
        <cd:Mapping string="TRUE" value="1"/>
        <cd:Mapping string="True" value="1"/>
        <cd:Mapping string="true" value="1"/>
        <ct:NamedTest id="BooleanXmlTest"/>
      </cd:Integer>
    </cd:Type>
  </cd:Field>
</cd:Record>