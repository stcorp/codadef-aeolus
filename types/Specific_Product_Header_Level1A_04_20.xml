<?xml version="1.0"?>
<cd:Record xmlns:cd="http://www.stcorp.nl/coda/definition/2008/07" xmlns:ct="http://www.stcorp.nl/coda/test/2008/10" format="xml" last-modified="2025-01-16" name="Specific_Product_Header_Level1A_04_20" namexml="Specific_Product_Header">
  <cd:Description>Specific Product Header for ALD Level 1A products</cd:Description>
  <cd:Field name="Sph_Descriptor">
    <cd:Text namexml="Sph_Descriptor">
    </cd:Text>
  </cd:Field>
  <cd:Field name="DOI">
    <cd:Text namexml="DOI">
    </cd:Text>
  </cd:Field>
  <cd:Field name="Intersect_Start_Lat">
    <cd:Type namexml="Intersect_Start_Lat">
      <cd:Attribute name="unit">
        <cd:Optional/>
        <cd:FixedValue>10-6DegN</cd:FixedValue>
      </cd:Attribute>
      <cd:Integer format="ascii">
        <cd:Description>Latitude of the intersection of WGS84 DEM and the satellite line-of-sight for the first measurement (first measurement centroid time of the first BRC)</cd:Description>
        <cd:Unit>1e-6 degrees_north</cd:Unit>
        <cd:Conversion denominator="1000000.000000" numerator="1.000000">
          <cd:Unit>degrees_north</cd:Unit>
        </cd:Conversion>
        <cd:NativeType>int32</cd:NativeType>
        <ct:NamedTest id="RangeLatitude6"/>
      </cd:Integer>
    </cd:Type>
  </cd:Field>
  <cd:Field name="Intersect_Start_Long">
    <cd:Type namexml="Intersect_Start_Long">
      <cd:Attribute name="unit">
        <cd:Optional/>
        <cd:FixedValue>10-6DegE</cd:FixedValue>
      </cd:Attribute>
      <cd:Integer format="ascii">
        <cd:Description>Longitude of the intersection of WGS84 DEM and the satellite line-of-sight for the first measurement (first measurement centroid time of the first BRC)</cd:Description>
        <cd:Unit>1e-6 degrees_east</cd:Unit>
        <cd:Conversion denominator="1000000.000000" numerator="1.000000">
          <cd:Unit>degrees_east</cd:Unit>
        </cd:Conversion>
        <cd:NativeType>int32</cd:NativeType>
        <ct:NamedTest id="RangeLongitude6"/>
      </cd:Integer>
    </cd:Type>
  </cd:Field>
  <cd:Field name="Intersect_Stop_Lat">
    <cd:Type namexml="Intersect_Stop_Lat">
      <cd:Attribute name="unit">
        <cd:Optional/>
        <cd:FixedValue>10-6DegN</cd:FixedValue>
      </cd:Attribute>
      <cd:Integer format="ascii">
        <cd:Description>Latitude of the intersection of WGS84 DEM and the satellite line-of-sight for the last measurement (last measurement centroid time of the last BRC)</cd:Description>
        <cd:Unit>1e-6 degrees_north</cd:Unit>
        <cd:Conversion denominator="1000000.000000" numerator="1.000000">
          <cd:Unit>degrees_north</cd:Unit>
        </cd:Conversion>
        <cd:NativeType>int32</cd:NativeType>
        <ct:NamedTest id="RangeLatitude6"/>
      </cd:Integer>
    </cd:Type>
  </cd:Field>
  <cd:Field name="Intersect_Stop_Long">
    <cd:Type namexml="Intersect_Stop_Long">
      <cd:Attribute name="unit">
        <cd:Optional/>
        <cd:FixedValue>10-6DegE</cd:FixedValue>
      </cd:Attribute>
      <cd:Integer format="ascii">
        <cd:Description>Longitude of the intersection of WGS84 DEM and the satellite line-of-sight for the last measurement (last measurement centroid time of the last BRC)</cd:Description>
        <cd:Unit>1e-6 degrees_east</cd:Unit>
        <cd:Conversion denominator="1000000.000000" numerator="1.000000">
          <cd:Unit>degrees_east</cd:Unit>
        </cd:Conversion>
        <cd:NativeType>int32</cd:NativeType>
        <ct:NamedTest id="RangeLongitude6"/>
      </cd:Integer>
    </cd:Type>
  </cd:Field>
  <cd:Field name="Sat_Track">
    <cd:Type namexml="Sat_Track">
      <cd:Attribute name="unit">
        <cd:Optional/>
        <cd:FixedValue>deg</cd:FixedValue>
      </cd:Attribute>
      <cd:Float format="ascii">
        <cd:Description>Sub-satellite track heading (nadir) at the sensing start time in the MPH</cd:Description>
        <cd:Unit>degrees</cd:Unit>
        <cd:NativeType>double</cd:NativeType>
      </cd:Float>
    </cd:Type>
  </cd:Field>
  <cd:Field name="Base_Laser_Frequency">
    <cd:Type namexml="Base_Laser_Frequency">
      <cd:Attribute name="unit">
        <cd:Optional/>
        <cd:FixedValue>GHz</cd:FixedValue>
      </cd:Attribute>
      <cd:Float format="ascii">
        <cd:Description>Base Laser Transmit Frequency</cd:Description>
        <cd:Unit>GHz</cd:Unit>
        <cd:NativeType>double</cd:NativeType>
      </cd:Float>
    </cd:Type>
  </cd:Field>
  <cd:Field name="Spare_1">
    <cd:Hidden/>
    <cd:Text namexml="Spare_1">
    </cd:Text>
  </cd:Field>
  <cd:Field name="Total_Num_of_Observations">
    <cd:Type namexml="Total_Num_of_Observations">
      <cd:Integer format="ascii">
        <cd:Description>Number of observations in the product</cd:Description>
        <cd:NativeType>int32</cd:NativeType>
      </cd:Integer>
    </cd:Type>
  </cd:Field>
  <cd:Field name="Num_Valid_Observations">
    <cd:Type namexml="Num_Valid_Observations">
      <cd:Integer format="ascii">
        <cd:Description>Number of valid observations in the product</cd:Description>
        <cd:NativeType>int32</cd:NativeType>
      </cd:Integer>
    </cd:Type>
  </cd:Field>
  <cd:Field name="N_Max">
    <cd:Type namexml="N_Max">
      <cd:Integer format="ascii">
        <cd:Description>The maximum number of measurements per BRC in the output Level 1A product. Note the measurement array dimension in the L1A product is a constant value, N = 67</cd:Description>
        <cd:NativeType>int32</cd:NativeType>
      </cd:Integer>
    </cd:Type>
  </cd:Field>
  <cd:Field name="Spare_2">
    <cd:Hidden/>
    <cd:Text namexml="Spare_2">
    </cd:Text>
  </cd:Field>
  <cd:Field name="Total_Num_of_Measurements">
    <cd:Type namexml="Total_Num_of_Measurements">
      <cd:Integer format="ascii">
        <cd:Description>Number of measurements in the product (sum of all N over the all the BRCs)</cd:Description>
        <cd:NativeType>int32</cd:NativeType>
      </cd:Integer>
    </cd:Type>
  </cd:Field>
  <cd:Field name="Num_Measurement_Data_Present">
    <cd:Type namexml="Num_Measurement_Data_Present">
      <cd:Integer format="ascii">
        <cd:Description>Number of measurements present (= number of measurement SIDs NOT missing)</cd:Description>
        <cd:NativeType>int32</cd:NativeType>
      </cd:Integer>
    </cd:Type>
  </cd:Field>
  <cd:Field name="Num_Mie_Measurement_Sp_Valid">
    <cd:Type namexml="Num_Mie_Measurement_Sp_Valid">
      <cd:Integer format="ascii">
        <cd:Description>Number of Valid Mie Measurement Source Packets</cd:Description>
        <cd:NativeType>int32</cd:NativeType>
      </cd:Integer>
    </cd:Type>
  </cd:Field>
  <cd:Field name="Num_Rayleigh_Measurement_Sp_Valid">
    <cd:Type namexml="Num_Rayleigh_Measurement_Sp_Valid">
      <cd:Integer format="ascii">
        <cd:Description>Number of Valid Rayleigh Measurement Source Packets</cd:Description>
        <cd:NativeType>int32</cd:NativeType>
      </cd:Integer>
    </cd:Type>
  </cd:Field>
  <cd:Field name="Num_Measurement_Valid">
    <cd:Type namexml="Num_Measurement_Valid">
      <cd:Integer format="ascii">
        <cd:Description>Number of measurements for which the number of corresponding valid pulses is over Meas_Cavity_Lock_Status_Thresh and pulse is valid</cd:Description>
        <cd:NativeType>int32</cd:NativeType>
      </cd:Integer>
    </cd:Type>
  </cd:Field>
  <cd:Field name="Num_Measurement_Sat_on_Target">
    <cd:Type namexml="Num_Measurement_Sat_on_Target">
      <cd:Integer format="ascii">
        <cd:Description>Number of measurements with satellite on target status</cd:Description>
        <cd:NativeType>int32</cd:NativeType>
      </cd:Integer>
    </cd:Type>
  </cd:Field>
  <cd:Field name="Spare_3">
    <cd:Hidden/>
    <cd:Text namexml="Spare_3">
    </cd:Text>
  </cd:Field>
  <cd:Field name="Total_Num_of_Reference_Pulses">
    <cd:Type namexml="Total_Num_of_Reference_Pulses">
      <cd:Integer format="ascii">
        <cd:Description>Number of reference pulses in the product (sum of all P over the all the BRCs)</cd:Description>
        <cd:NativeType>int32</cd:NativeType>
      </cd:Integer>
    </cd:Type>
  </cd:Field>
  <cd:Field name="Num_Mie_Reference_Pulses_Present">
    <cd:Type namexml="Num_Mie_Reference_Pulses_Present">
      <cd:Integer format="ascii">
        <cd:Description>Number of Mie Reference Pulses present in the product (= number of SID3 not missing)</cd:Description>
        <cd:NativeType>int32</cd:NativeType>
      </cd:Integer>
    </cd:Type>
  </cd:Field>
  <cd:Field name="Num_Rayleigh_Reference_Pulses_Present">
    <cd:Type namexml="Num_Rayleigh_Reference_Pulses_Present">
      <cd:Integer format="ascii">
        <cd:Description>Number of Rayleigh Reference Pulses present in BRC (= number of SID4 not missing)</cd:Description>
        <cd:NativeType>int32</cd:NativeType>
      </cd:Integer>
    </cd:Type>
  </cd:Field>
  <cd:Field name="Num_Mie_Reference_Pulse_Sp_Valid">
    <cd:Type namexml="Num_Mie_Reference_Pulse_Sp_Valid">
      <cd:Integer format="ascii">
        <cd:Description>Number of valid Mie Reference Pulse Source Packets</cd:Description>
        <cd:NativeType>int32</cd:NativeType>
      </cd:Integer>
    </cd:Type>
  </cd:Field>
  <cd:Field name="Num_Rayleigh_Reference_Pulse_Sp_Valid">
    <cd:Type namexml="Num_Rayleigh_Reference_Pulse_Sp_Valid">
      <cd:Integer format="ascii">
        <cd:Description>Number of validRayleigh Reference Pulse Source Packets</cd:Description>
        <cd:NativeType>int32</cd:NativeType>
      </cd:Integer>
    </cd:Type>
  </cd:Field>
  <cd:Field name="Num_Reference_Pulse_Valid">
    <cd:Type namexml="Num_Reference_Pulse_Valid">
      <cd:Integer format="ascii">
        <cd:Description>Number of reference pulses with valid status</cd:Description>
        <cd:NativeType>int32</cd:NativeType>
      </cd:Integer>
    </cd:Type>
  </cd:Field>
  <cd:Field name="Spare_4">
    <cd:Hidden/>
    <cd:Text namexml="Spare_4">
    </cd:Text>
  </cd:Field>
  <cd:Field name="List_of_Dsds">
    <cd:NamedType id="List_of_Dsds"/>
  </cd:Field>
</cd:Record>
